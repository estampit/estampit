{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Next.js Web (Dev)",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run", "web"],
      "console": "integratedTerminal",
      "skipFiles": ["<node_internals>/**"]
    },
    {
      "name": "Supabase: Local Stack Attach",
      "type": "node",
      "request": "attach",
      "port": 9229,
      "restart": true,
      "timeout": 10000
    }
  ]
}
